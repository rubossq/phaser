<?xml version='1.0' encoding='utf-8'?>
<widget id="com.wayne.startypego" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>StarType Go</name>
    <description>
        Star type go
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        StarType Go
    </author>
    
    <preference name="orientation" value="portrait" />
    <preference name="BackgroundColor" value="0xff0f1321" />
    <preference name="android-minSdkVersion" value="19" />
    <preference name="Fullscreen" value="true" />

    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    
    <icon height="36" platform="ios" src="icons/icon-36-ldpi.png" width="36" />
    <icon height="48" platform="ios" src="icons/icon-48-mdpi.png" width="48" />
    <icon height="72" platform="ios" src="icons/icon-72-hdpi.png" width="72" />
    <icon height="96" platform="ios" src="icons/icon-96-xhdpi.png" width="96" />
    <icon height="144" platform="ios" src="icons/icon-144-xxhdpi.png" width="144" />

    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
